{
  "_version": "1.65.0",
  "sap.app": {
    "id": "com.ui5.trng.sapui5bootcampactivity04",
    "type": "application",
    "i18n": "i18n/i18n.properties",
    "applicationVersion": {
      "version": "0.0.1"
    },
    "title": "{{appTitle}}",
    "description": "{{appDescription}}",
    "resources": "resources.json",
    "sourceTemplate": {
      "id": "@sap/generator-fiori:basic",
      "version": "1.18.4",
      "toolsId": "ea79efe6-1908-4946-b52a-de4d888a6411"
    }
  },
  "sap.ui": {
    "technology": "UI5",
    "icons": {
      "icon": "",
      "favIcon": "",
      "phone": "",
      "phone@2": "",
      "tablet": "",
      "tablet@2": ""
    },
    "deviceTypes": {
      "desktop": true,
      "tablet": true,
      "phone": true
    }
  },
  "sap.ui5": {
    "flexEnabled": true,
    "dependencies": {
      "minUI5Version": "1.139.0",
      "libs": {
        "sap.m": {},
        "sap.ui.core": {}
      }
    },
    "contentDensities": {
      "compact": true,
      "cozy": true
    },
    "models": {
      "i18n": {
        "type": "sap.ui.model.resource.ResourceModel",
        "settings": {
          "bundleName": "com.ui5.trng.sapui5bootcampactivity04.i18n.i18n"
        }
      }
    },
    "resources": {
      "css": [
        {
          "uri": "css/style.css"
        }
      ]
    },
    "routing": {
      "config": {
        "routerClass": "sap.m.routing.Router",
        "controlAggregation": "pages",
        "controlId": "app",
        "transition": "slide",
        "type": "View",
        "viewType": "XML",
        "path": "com.ui5.trng.sapui5bootcampactivity04.view",
        "async": true,
        "viewPath": "com.ui5.trng.sapui5bootcampactivity04.view"
      },
      "routes": [
        {
          "name": "MainView",
          "pattern": ":?query:",
          "target": [
            "mainView"
          ]
        }
      ],
      "targets": {
        "mainView": {
          "id": "MainView",
          "name": "MainView"
        }
      }
    },
    "rootView": {
      "viewName": "com.ui5.trng.sapui5bootcampactivity04.view.App",
      "type": "XML",
      "id": "App",
      "async": true
    }
  },
  "sap.cloud": {
    "public": true,
    "service": "comui5trngsapui5bootcampactivity04"
  }
}
